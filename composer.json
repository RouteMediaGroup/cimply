    "require": {
        "php": "^7.2.*" || ^8.0",
        "ext-pcre": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.20 || ^8.5.2 || ^9.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
