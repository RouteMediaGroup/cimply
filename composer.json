{
    "name": "routemediagroup/cimply",
    "description": "Cimply.Work is a PHP-Tool to creating business-solutions for digitization and automation.",
    "keywords": ["framework"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "RouteMedia",
            "email": "direkt@route-media.info"
	},
	{
	    "name": "Cimply.Work",
	    "homepage": "http://cimply.work"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Cimply\\": "cimply/"
